{
  "name": "github-app",
  "version": "0.0.0-development",
  "private": true,
  "description": "GitHub App template repository",
  "repository": "github:bfra-me/github-app",
  "license": "MIT",
  "author": "Marcus R. Brown <contact@marcusrbrown.com>",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "bootstrap": "pnpm install --prefer-offline --loglevel warn",
    "build": "NODE_ENV=production tsup",
    "check-format": "prettier --check .",
    "fix": "pnpm run lint --fix",
    "format": "prettier --write .",
    "lint": "eslint .",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@bfra.me/prettier-config/120-proof",
  "dependencies": {
    "@probot/adapter-github-actions": "4.0.3",
    "probot": "13.4.7"
  },
  "devDependencies": {
    "@bfra.me/eslint-config": "0.24.0",
    "@bfra.me/prettier-config": "0.15.5",
    "@bfra.me/tsconfig": "0.10.0",
    "@octokit/webhooks": "^14.0.0",
    "@octokit/webhooks-types": "^7.4.0",
    "@semantic-release/git": "10.0.1",
    "@types/node": "^22.0.0",
    "@vitest/eslint-plugin": "1.3.4",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-prettier": "5.5.3",
    "jiti": "2.5.1",
    "nock": "14.0.6",
    "prettier": "3.6.2",
    "semantic-release": "24.2.7",
    "tsup": "8.5.0",
    "typescript": "5.8.3",
    "vitest": "^3.0.0"
  },
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": "^16.20.0 || >=18"
  }
}
